{
  "plugins": [
        "plugins/markdown"
  ],
  "recurseDepth": 10,
  "opts": {
    "recurse": true,
    "destination": "./docs/",
     "template": "node_modules/docdash",
     "encoding": "utf8",
      "readme": "./README.md",
      "destination": "docs/",
      "verbose": true,
      "theme_opts": {
            "theme": "dark"
        }
  },

  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}